\hbadness=10000
\hfuzz=\maxdimen

\def\p#1{#1\setbox0\vbox{\hsize0pt #1}}

\directlua{\unexpanded{
  local words = io.open('words.txt')
  for w in words:lines() do
    tex.print('\\p{' .. w .. '}')
  end
  words:close()
}}

