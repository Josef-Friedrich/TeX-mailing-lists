		char = slot('fl'),
		type = 0
	}
}

characters[slot('ff')].ligatures = {
	[slot('i')] = { 	
		char = slot('ffi'),
		type = 0
	},
	[slot('l')] = { 	
		char = slot('ffl'),
		type = 0
	}
}

