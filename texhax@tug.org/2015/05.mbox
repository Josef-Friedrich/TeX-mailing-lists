@book{AW.Brooks.2011-Design-of-Design,
     author   ={Frederick P. {Brooks Jr.}},
     title    ={The Design of Design},
     publisher={Addison-Wesley},
     year     = 2010,
     note     ={421 pp., 28 chap., index: 20 pp.;
     history of the design process in many disciplines,
     besides software},
     url      =
{
http://www.pearsonhighered.com/educator/product/Design-of-Design-The-Essays-from-a-Computer-Scientist/9780201362985.page
