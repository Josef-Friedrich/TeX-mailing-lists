\def\pgfsys at functionalshading#1#2#3#4{%
  {%
    \pgf at process{#2}%
    \pgf at xa=\pgf at x%
    \pgf at ya=\pgf at y%
    \pgf at process{#3}%
    \pgf at xb=\pgf at x%
    \pgf at yb=\pgf at y%
