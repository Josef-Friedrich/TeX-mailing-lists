--- TLPOBJ.pm	(revision 11703)
+++ TLPOBJ.pm	(working copy)
@@ -509,20 +509,36 @@
               ? () : ("--owner", "0", "--group", "0");
   my @cmdline = ($tar, "-cf", "$destdir/$tarname", @attrs);
   
-  # Get list of files.  We have to pass them all on the command line,
