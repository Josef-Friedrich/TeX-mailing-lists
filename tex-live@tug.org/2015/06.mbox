\parskip = 0 pt
\parindent = 0 pt
\let \\ = \par

\newdimen \labelwidth
\newdimen \labelheight
\newdimen \labelhsep
\newdimen \labelvsep

\labelwidth = 3 in
\labelheight = 2 in
\labelvsep = 1 cm

\def \newlabel #1{\vbox to \labelheight {\hsize = \labelwidth #1\\}}

\def \newlabelpair #1#2%
    {%
	\setbox 0 = \vbox
	    {\newlabel {#1}\vskip \labelvsep \newlabel {#2}}%
	\special {pdf:literal direct  0 1 -1 0  0 -500 cm}
	\unvbox 0
	\eject
    }
						
