gcc -DHAVE_CONFIG_H  -I. -I. -I.. -I./../..  -I.. -I./.. -I../.. -I./../.. -DHAVE_TIFF -I../../../libs/libtiff -I./../../../libs/libtiff -I../../../libs/libpng -I./../../../libs/libpng -I../../../libs/zlib -I./../../../libs/zlib -DpdfTeX -O2  -c
